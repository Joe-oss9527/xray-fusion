{
  "metadata": {
    "id": "office",
    "name": "Office/Team",
    "description": "Optimized for small office or team use with multiple users",
    "category": "business",
    "author": "xray-fusion",
    "version": "1.0.0",
    "min_xray_version": "v1.8.0"
  },
  "config": {
    "topology": "vision-reality",
    "xray": {
      "version": "latest",
      "vision_port": 8443,
      "reality_port": 443,
      "fallback_port": 8080,
      "sni": "www.cloudflare.com",
      "reality_dest": "www.cloudflare.com:443",
      "sniffing": true
    },
    "plugins": [
      "cert-auto",
      "firewall"
    ],
    "security": {
      "level": "high",
      "features": {
        "tls13_only": true,
        "cert_pinning": true,
        "firewall_rules": true
      }
    },
    "performance": {
      "max_connections": 500,
      "buffer_size": "large"
    }
  },
  "requirements": [
    "Domain ownership required for vision-reality topology",
    "Ports 443, 8443, 8080 must be available",
    "Automatic certificate management enabled"
  ],
  "notes": [
    "Suitable for 5-20 users",
    "Requires domain ownership",
    "High security settings",
    "Auto-managed TLS certificates"
  ]
}
